# service libs
pod 'SwiftyTimer'
pod 'SwiftDate'

# feature libs
pod 'JSQMessagesViewController'

# UI libs
pod 'Neon'
pod 'TextFieldEffects'

# pubsub libs
pod 'Signals'

# event handling
pod 'PromiseKit'

# view libs
pod 'Neon'

# image libs
pod 'HanekeSwift'
pod 'Toucan'

# data storage libs
pod 'RealmSwift'

# onboarding libs
pod 'Onboard'


#####################################
